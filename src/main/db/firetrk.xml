<?xml version="1.0" encoding="utf-8" standalone="no"?>
<!DOCTYPE hi2txt SYSTEM "hi2txt.dtd">
<hi2txt label="Fire Truck">
  <!--
    tested with mame 0.181 and embedded hiscore.dat
    source: GreatStone
  //-->

  <structure file=".hi">
    <check>
	  <definition>@:maincpu,program,28,1,06,06</definition>
      <size>1</size>
    </check>
	<loop count="1">
      <elt size="6" type="int" id="SCORE" endianness="little_endian"/>
	</loop>
  </structure>

  <output>
    <table>
      <column id="SCORE" format="*10"/>
	</table>
  </output>
</hi2txt>