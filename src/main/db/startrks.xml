<?xml version="1.0" encoding="utf-8" standalone="no"?>
<!DOCTYPE hi2txt SYSTEM "hi2txt.dtd">
<hi2txt label="Star Trek (Head On hardware)">
    <!--
      tested with mame 0229 and embedded hiscore.dat
      source: GreatStone
    //-->

    <structure file=".hi">
        <check>
            <definition>@:maincpu,program,d78a,5,30,30</definition>
            <size>5</size>
        </check>
        <loop count="1">
          <elt size="5" type="text" id="SCORE"/>
        </loop>
    </structure>

    <output>
        <table>
            <column id="RANK" src="index" format="+1"/>
            <column id="SCORE" format="TrimL0"/>
        </table>
    </output>
</hi2txt>