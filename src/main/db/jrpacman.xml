<?xml version="1.0" encoding="utf-8" standalone="no"?>
<!DOCTYPE hi2txt SYSTEM "hi2txt.dtd">
<hi2txt label="Jr. Pac-Man (11/9/83)">
  <!--
    tested with mame 0.181 and embedded hiscore.dat
    source: GreatStone
  //-->
  
  <structure file=".hi">
    <check>
      <!-- optional: if defined and hiscore.dat provided, it allows to select the good structure versus the provided file //-->
      <definition>@:maincpu,program,4751,01,48,48
                  @:maincpu,program,476d,06,40,40
                  @:maincpu,program,4e88,03,00,00</definition>
      <!-- optional: if defined, it allows to select the good structure versus the provided file
                     useful if multiple structures are defined but hiscore.dat not provided //-->
      <size>10</size>
    </check>
    <loop count="1">
      <elt size="1" type="raw"  id="UNKNOWN"/>
	  <elt size="6" type="int"  id="SCORE2" decoding-profile="bcd-le"/>
      <elt size="3" type="int"  id="SCORE"  endianness="little_endian" base="16"/>
    </loop>
  </structure>
  
  <output>
    <table>
      <column id="RANK" src="index" format="+1"/>
      <column id="SCORE"/>
	  <column id="SCORE2"              display="debug"/>
      <column id="UNKNOWN" format="0x" display="debug"/>
    </table>
  </output>
</hi2txt>
