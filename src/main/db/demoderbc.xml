<?xml version="1.0" encoding="utf-8" standalone="no"?>
<!DOCTYPE hi2txt SYSTEM "hi2txt.dtd">
<hi2txt label="Demolition Derby">
  <!--
    tested with mame 0.181 and embedded hiscore.dat
    source: GreatStone
  //-->

  <structure file="nvram">
    <check>
      <size>2048</size>
    </check>
	<elt size="1262" type="raw"  id="UNKNOWN"/>
    <loop count="5">
      <elt size="3" type="text" id="NAME BLUE"/>
    </loop>
	<loop count="5">
      <elt size="3" type="text" id="NAME RED"/>
    </loop>
	<loop count="5">
      <elt size="3" type="text" id="NAME YELLOW"/>
    </loop>
	<loop count="5">
      <elt size="3" type="text" id="NAME GREEN"/>
    </loop>
    <elt size="16" type="raw"  id="UNKNOWN"/>
    <loop count="5">
      <elt size="6" type="int" id="SCORE BLUE" base="256"/>
    </loop>
	<loop count="5">
      <elt size="6" type="int" id="SCORE RED" base="256"/>
    </loop>
	<loop count="5">
      <elt size="6" type="int" id="SCORE YELLOW" base="256"/>
    </loop>
	<loop count="5">
      <elt size="6" type="int" id="SCORE GREEN" base="256"/>
    </loop>
  </structure>

  <output>
    <table>
      <column id="RANK" src="index" format="+1"/>
	  <column id="SCORE BLUE"/>
      <column id="NAME BLUE"/>
	  <column id="SCORE RED"/>
	  <column id="NAME RED"/>
	  <column id="SCORE YELLOW"/>
	  <column id="NAME YELLOW"/>
      <column id="SCORE GREEN"/>
	  <column id="NAME GREEN"/>
	</table>
  </output>
</hi2txt>